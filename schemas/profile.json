{
    "schema": {
        "id": "profile",
        "properties": {
            "account": {
                "id": "account",
                "pattern": "[\\w]+\\.[\\w]+\\.[\\w]+",
                "type": "string"
            },
            "birthday": {
                "id": "birthday",
                "pattern": "^(-?(?:[1-9][0-9]*)?[0-9]{4})-(1[0-2]|0[1-9])-(3[01]|0[1-9]|[12][0-9])T(2[0-3]|[01][0-9]):([0-5][0-9]):([0-5][0-9])(\\.[0-9]+)?(Z|[+-](?:2[0-3]|[01][0-9]):[0-5][0-9])?$",
                "type": "string"
            },
            "firstName": {
                "id": "firstName",
                "pattern": "^\\w{3,64}",
                "type": "string"
            },
            "lastName": {
                "id": "lastName",
                "pattern": "^\\w{3,64}",
                "type": "string"
            },
            "sessions": {
                "id": "sessions",
                "items": {
                    "pattern": "[\\w]+\\.[\\w]+\\.[\\w]+",
                    "type": "string"
                },
                "type": "array",
                "uniqueItems": true
            }
        },
        "required": [
            "firstName",
            "lastName",
            "birthday",
            "account"
        ],
        "type": "object"
    },
    "type": "jsonschema"
}